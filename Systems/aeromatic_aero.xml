          <property>aero/qbar-psf</property>
          <property>metrics/Sw-sqft</property>
          <table>
            <independentVar lookup="row">aero/alpha-rad</independentVar>
            <tableData>
              -0.20 -0.740
               0.00 0.240
             0.24    1.400
               0.60 0.704
            </tableData>
          </table>
      </product>
    </function>

    <function name="aero/coefficient/dCLflap">
