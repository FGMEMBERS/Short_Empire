 <!-- Animations for the control columns. -->
 <animation>
  <type>rotate</type>
  <object-name>control_column_L</object-name>
  <object-name>yoke_L</object-name>
  <object-name>control_column_R</object-name>
  <object-name>yoke_R</object-name>
  <property>surface-positions/elevator-pos-norm</property>
  <factor>15.67</factor>
  <offset-deg>0.0</offset-deg>
  <center>
   <x-m> -2.00 </x-m>
   <y-m>  0.00 </y-m>
   <z-m>  0.80 </z-m>
  </center>
  <axis>
   <x> 0.0 </x>
