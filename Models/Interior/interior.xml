<PropertyList>

 <path>interior.ac</path>
 <offsets>
  <!-- x/y/z == forward/left/up -->
  <x-m> 0.0 </x-m>
  <y-m> 0.0 </y-m>
  <z-m> 0.0 </z-m>
 </offsets>

 <!-- Instrument panel =====================================================-->
 <model>
  <name>Panel instruments</name>
  <path>Aircraft/Short_Empire/Models/Interior/instrument_panel.xml</path>
  <offsets>
   <heading-deg>180</heading-deg>
   <!-- x/y/z == forward/left/up -->
   <x-m>-1.6500 </x-m>
   <y-m>  0.000 </y-m>
   <z-m>  1.348 </z-m>
   <pitch-deg>0.0</pitch-deg>
  </offsets>
 </model>

 <!-- Star observer's instruments -->
 <model>
  <name>Bubble_sextant</name>
  <path>Aircraft/Short_Empire/Instruments/BubbleSextant/RAF_Mk9_bubble_sextant.xml</path>
  <offsets>
   <heading-deg>180</heading-deg>
   <x-m> -2.50 </x-m>
   <y-m> -0.50 </y-m>
   <z-m>  2.00 </z-m>
  </offsets>
 </model>

 <!-- Engine instrument panel ==============================================-->
 <model>
  <name>Engine instrument panel</name>
  <path>Aircraft/Short_Empire/Models/Interior/engine_panel.xml</path>
  <offsets>
   <!-- x/y/z == forward/left/up -->
   <x-m> -3.100 </x-m>
   <y-m> -1.080 </y-m>
   <z-m>  1.100 </z-m>
   <heading-deg>99.0</heading-deg>
   <pitch-deg>2.0</pitch-deg>
  </offsets>
 </model>

 <!-- Spar panel ===========================================================-->
 <model>
  <name>Instruments at the spar bulkhead</name>
  <path>Aircraft/Short_Empire/Models/Interior/spar_panel.xml</path>
  <offsets>
   <!-- x/y/z == forward/left/up -->
   <x-m> -8.200 </x-m>
   <y-m>  0.000 </y-m>
   <z-m>  1.100 </z-m>
   <pitch-deg>0.0</pitch-deg>
  </offsets>
 </model>

