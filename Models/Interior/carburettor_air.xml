<PropertyList>

 <parameters>
  <lighting-norm>controls/lighting/dome-norm</lighting-norm>
 </parameters>

 <path>fuel_pump_test_cocks.ac</path>
 <offsets>
  <!-- x/y/z == forward/left/up -->
  <x-m> 0.0 </x-m>
  <y-m> 0.0 </y-m>
  <z-m> 0.0 </z-m>
 </offsets>

 <animation>
  <type>material</type>
  <object-name>back_plate</object-name>
  <texture>carburettor_air.png</texture>
  <emission> 
   <factor-prop alias="../../../parameters/lighting-norm"/>
   <red>0.75</red>
   <green>0.25</green>
   <blue>0.25</blue>
  </emission>
 </animation>

</PropertyList>
